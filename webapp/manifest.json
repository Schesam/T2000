{
	"_version": "1.12.0",
	"sap.app": {
		"id": "koehler.T2000",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "img/Iconby2672796852787429onfreeicons.io.ico",
			"favIcon": "img/Iconby2672796852787429onfreeicons.io.svg",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "koehler.T2000.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.71.18",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "koehler.T2000.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "koehler.T2000.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"viewLevel": 1,
				"transition": "fade",
				"bypassed": {
					"target": []
				}
			},
			"routes": [
				{
					"name": "Detail",
					"pattern": "Detail",
					"titleTarget": "Detail",
					"greedy": false,
					"target": [
						"Detail"
					]
				},
				{
					"name": "DetailP",
					"pattern": "Detail/:num:",
					"titleTarget": "Detail",
					"greedy": false,
					"target": [
						"Detail"
					]
				},
				{
					"name": "Selection",
					"pattern": "Selection",
					"titleTarget": "Selection",
					"greedy": false,
					"target": [
						"Selection"
					]
				},
				{
					"name": "SelectionP",
					"pattern": "Selection/:num:",
					"titleTarget": "Selection",
					"greedy": false,
					"target": [
						"Selection"
					]
				}
			],
			"targets": {
				"App": {
					"viewType": "XML",
					"viewName": "App"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "fade",
					"clearControlAggregation": false,
					"viewName": "Detail",
					"viewId": "Detail",
					"viewLevel": 1
				},
				"Selection": {
					"viewType": "XML",
					"viewName": "Selection",
					"transition": "fade"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}