<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="onFilterDialogConfirm" filterItems="{path:'/rows/Names', templateShareable:false}">
		<filterItems>
			<ViewSettingsFilterItem text="{Value}" key="{Key}" multiSelect="false" items="{path:'Items', templateShareable:false}">
				<items>
					<ViewSettingsItem text="{Name}" key="{Name}___{Key}"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>