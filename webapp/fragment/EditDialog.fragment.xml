<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="editDialog" title="{i18n>edit}" stretch="{device>/isPhone}">
		<content>
			<form:Form editable="true">
				<form:layout>
					<form:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" emptySpanXL="3" emptySpanL="3" emptySpanM="3"
						emptySpanS="0" columnsXL="4" columnsL="4" columnsM="4" adjustLabelSpan="true" breakpointXL="1440" breakpointL="1024" breakpointM="600"/>
				</form:layout>
				<form:formContainers>
					<form:FormContainer title="{i18n>formPersonData}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>employee}" width="100%"/>
								</form:label>
								<form:fields>
									<Select id="editEmployeeSelect" selectedKey="1">
										<core:ListItem text="Andreas KÃ¶hler" key="1"/>
										<core:ListItem text="Thomas Rempel" key="2"/>
										<core:ListItem text="Jurij Rudi" key="3"/>
										<core:ListItem text="Igor Aronov" key="4"/>
									</Select>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colBC}"/>
								</form:label>
								<form:fields>
									<ComboBox id="editBCCombo" items="{/rows}" required="true">
										<core:ListItem text="{Value}" key="{Key}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formBasisData}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colCategory}" width="100%"/>
								</form:label>
								<form:fields>
									<ComboBox id="editCategoryCombo" items="{/rows}" required="true">
										<core:ListItem text="{Value}" key="{Key}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colArea}"/>
								</form:label>
								<form:fields>
									<ComboBox id="editAreaCombo" items="{/rows}" required="true">
										<core:ListItem text="{Value}" key="{Key}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colTask}"/>
								</form:label>
								<form:fields>
									<ComboBox id="editTaskCombo" items="{/rows}" required="true">
										<core:ListItem text="{Value}" key="{Key}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formPlanningData}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colPlanned}"/>
								</form:label>
								<form:fields>
									<Switch id="editPlanned" type="AcceptReject"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colStatus}"/>
								</form:label>
								<form:fields>
									<ComboBox id="editStatusCombo" items="{/rows}" required="true">
										<core:ListItem text="{Value}" key="{Key}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colPriority}"/>
								</form:label>
								<form:fields>
									<StepInput id="editPriority" required="true"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formFurtherLinks}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colProject}"/>
								</form:label>
								<form:fields>
									<TextArea growing="true" rows="1" id="editProject" required="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colJira}"/>
								</form:label>
								<form:fields>
									<TextArea growing="true" rows="1" id="editJira" required="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colSpec}"/>
								</form:label>
								<form:fields>
									<TextArea growing="true" rows="1" id="editSpec" required="true"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formTimeData}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colCalendarWeek}"/>
								</form:label>
								<form:fields>
									<DatePicker id="editCalendarWeek" valueFormat="ww" displayFormat="ww" required="true"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>timeRange}"/>
								</form:label>
								<form:fields>
									<DateRangeSelection id="editTimerange" displayFormat="EEEE, dd. MMMM YYYY" valueFormat="dd.MM.YYYY" required="true"/>
								</form:fields>
							</form:FormElement>
							<!--<form:FormElement>-->
							<!--	<form:label>-->
							<!--		<Label text="{i18n>colRealBegin}"/>-->
							<!--	</form:label>-->
							<!--	<form:fields>-->
							<!--		<Input enabled="false" id="editRealBegin"/>-->
							<!--	</form:fields>-->
							<!--</form:FormElement>-->
							<!--<form:FormElement>-->
							<!--	<form:label>-->
							<!--		<Label text="{i18n>colRealEnd}"/>-->
							<!--	</form:label>-->
							<!--	<form:fields>-->
							<!--		<Input enabled="false" id="editRealEnd"/>-->
							<!--	</form:fields>-->
							<!--</form:FormElement>-->
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formSAPData}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>formSystem}"/>
								</form:label>
								<form:fields>
									<MultiInput id="editSystemMulti" required="true" showValueHelp="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>formTransport}"/>
								</form:label>
								<form:fields>
									<MultiInput id="editTransportMulti" required="true" showValueHelp="false"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
					<form:FormContainer title="{i18n>formComment}">
						<form:formElements>
							<form:FormElement>
								<form:label>
									<Label text="{i18n>colComment}"/>
								</form:label>
								<form:fields>
									<TextArea growing="true" required="true" id="editComment"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
				</form:formContainers>
			</form:Form>
		</content>
		<buttons>
			<Button id="saveButton" press="onSaveChanges" type="Accept" text="{i18n>save}"/>
			<Button id="cancelButton" press="onCancelChanges" type="Reject" text="{i18n>cancel}"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>